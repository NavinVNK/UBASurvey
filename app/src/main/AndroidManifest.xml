<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ubasurvey.nawin.com.ubasurvey">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="ubasurvey.nawin.com.ubasurvey.ChoiceApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/uba"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ChangePswActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".BasicinfoActivity"></activity>
        <activity android:name=".HouseholdActivity"></activity>

    </application>

</manifest>